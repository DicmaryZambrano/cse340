<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to home page">CSE Motors</a>
  </span>
  <div id="tools">
    <ul>
      <% if (locals.loggedin) { %>
        <li><a href="/account" title="Go to Dashboard">Welcome, <%= locals.accountData.account_firstname %></a></li>
        <li><a href="/account/logout" title="Click to log out">Logout</a></li>
        <% } else { %>
          <li><a href="/account/login" title="Click to log in">Login</a></li>
          <% } %>
    </ul>
  </div>
</header>